{
  "name": "nebula-feeder",
  "version": "1.0.0",
  "description": "Feeder from EOSWS to RabbitMQ",
  "main": "dist/server.js",
  "scripts": {
    "build-ts": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "postinstall": "npm run build-ts",
    "start": "npm run serve",
    "serve": "node dist/server.js",
    "dev": "tsc --watch & nodemon dist/server.js",
    "test": "jest src",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "8.11.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.1.3",
    "@types/node": "^10.12.0",
    "@types/ws": "^6.0.1"
  },
  "dependencies": {
    "@dfuse/eosws-js": "^0.11.5",
    "@types/bluebird": "^3.5.24",
    "amqp-ts": "^1.4.0",
    "amqplib": "^0.5.2",
    "dotenv": "^6.1.0"
  }
}
